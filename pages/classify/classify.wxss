.search {
  position: fixed;
  left: 0;
  top: 0;
  width: 100%;
  height: 8vh;
  display: flex;
  flex-direction: column;
  justify-content: center;
  background: #f3f5fa;
  padding: 0 34rpx;
  box-sizing: border-box;
  z-index: 999;
}

.search>.box {
  display: flex;
  align-items: center;
  padding: 15rpx 20rpx;
  border-radius: 100rpx;
  background: #fff;
  font-size: 25rpx;
}

.search>.box>.img {
  width: 44rpx;
  height: 44rpx;
  margin-right: 10rpx;
}

.search>.box>.img2 {
  width: 38rpx;
  height: 38rpx;
  padding-left: 10rpx;
}

.search>.box>.ipt {
  flex: 1;
}

.search>.box .input {
  width: 100%;
}

.search>.box .placeholder {
  color: #A7A7A7;
}

.content {
  height: 94vh;
  display: flex;
  justify-content: space-between;
}

.tab {
  position: fixed;
  left: 0;
  top: 8vh;
  height: 94vh;
  width: 164rpx;
  background: #f3f5fa;
  color: #6E6E6E;
  overflow-y: auto;
  padding-bottom: 100rpx;
  z-index: 999;
}

.tab>.item {
  text-align: center;
  height: 8vh;
  line-height: 8vh;
}

.tab>.sel {
  text-align: center;
  color: #F93C4C;
  background: #fff;
  font-weight: 800;
}

.two {
  position: fixed;
  left: 0;
  top: 8vh;
  width: 100%;
  height: 8vh;
  padding-left: 181rpx;
  font-size: 25rpx;
  color: #6E6E6E;
  background: #fff;
  line-height: 7vh;
  border-bottom: 1rpx solid #ececec;
  box-sizing: border-box;
  display: flex;
  align-items: center;
  overflow-x: auto;
  z-index: 998;
  display: flex;
  align-items: center;
  overflow-x: auto;
}

.two>.item {
  flex-basis: 140rpx;
  flex-shrink: 0;
  text-align: center;
}

.two>.sel {
  color: #F93C4C;
}

.list{
  margin-top: 16vh;
  padding-left: 181rpx;
  padding-right: 17rpx;
}

.list>.item {
  padding: 20rpx 0;
  border-bottom: 1rpx solid #ececec;
}

.list>.over {
  text-align: center;
  padding-top: 20rpx;
  color: #aaa;
  height: 200rpx;
}
